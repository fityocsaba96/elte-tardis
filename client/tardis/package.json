{
  "name": "tardis",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build --prod",
    "debug": "ng test --code-coverage --browsers Chrome",
    "test": "ng test --watch=false --code-coverage --browsers Chrome",
    "test-travis": "ng test --watch=false --browsers ChromeHeadlessNoSandbox",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start-client-server": "concurrently \"ng serve --open --proxy-config proxy.conf.json\" \"php -S 127.0.0.1:8000 -t ../../server/tardis-proxy/src\"",
    "startup": "npm install && npm run start-client-server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^6.0.0",
    "core-js": "^2.4.1",
    "ng2-semantic-ui": "^0.9.7",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "~6.0.2",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^6.0.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~10.1.0",
    "codelyzer": "^4.0.1",
    "concurrently": "^3.5.1",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^1.0.0",
    "protractor": "~5.3.1",
    "ts-node": "~6.0.0",
    "tslint": "~5.9.1",
    "tslint-sonarts": "^1.6.0",
    "typescript": "~2.8.1"
  }
}
